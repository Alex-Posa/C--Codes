#include <stdio.h>

int main()
{
    int Zahl;
    int eingabe;
    float i = 1;
    int j;
    int k = 1;

    scanf("%d",&eingabe);
    Zahl= i/k;

    for(j=0; j<= eingabe; j++){
        for(k=1; k <= eingabe;k++){
            if(k%2==0){
                printf("%f/%d+",i,j);
            } else if (k%2 != 0){
                printf("%f/%d-",i,j);
            }
        }

    }

    return 0;
}

